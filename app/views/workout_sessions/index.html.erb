<p style="color: green"><%= notice %></p>

<h1>Workout sessions</h1>

<div id="workout_sessions">
  <% @workout_sessions.each do |workout_session| %>
    <%= render workout_session %>
    <p>
      <%= link_to "Show this workout session", workout_session %>
    </p>
  <% end %>
</div>

<%= link_to "New workout session", new_workout_session_path %>
