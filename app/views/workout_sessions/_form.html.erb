  </div>

  <div>
    <%= form.label :description, style: "display: block" %>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.label :fees, style: "display: block" %>
    <%= form.text_field :fees %>
  </div>

  <div>
    <%= form.label :difficulty_level, style: "display: block" %>
