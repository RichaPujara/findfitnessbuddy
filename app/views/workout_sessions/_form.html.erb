    <%= form.label :duration, style: "display: block" %>
    <%= form.number_field :duration %>
  </div>

  <div>
    <%= form.label :workout_type, style: "display: block" %>
    <%= form.text_field :workout_type %>
  </div>

  <div>
    <%= form.label :date, style: "display: block" %>
