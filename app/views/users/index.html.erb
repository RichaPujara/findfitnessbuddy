<h1> All users </h1>
<div>
    <% @users.each do |user| %>
        <strong> Name: </strong> <%= "#{user.first_name} #{user.last_name}" %> <br>
        <strong> Email: </strong> <%= "#{user.email}" %> <br>
        <strong> Roles: </strong> <%= "#{user.roles.pluck(:name)}" %><br>
    <% end %>
</div>
